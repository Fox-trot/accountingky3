				СтрокНаЛисте = СтрокНаЛисте + 1; 
				
				// проверка на то что осталось еще вывести пустые строки
				// чтобы не было чистых мест на листах отчета
				
				ОсталостьСтрок = СтрокНаЛисте - ВсегоВыведеноСтрок;
				
			ИначеЕсли Счетчик > 27 Тогда //14 Тогда 
				Счетчик = 0;
				СтрокНаЛисте = 0;
				НомерЛиста = НомерЛиста + 1;
				
				СтруктураПараметров = Новый Структура;
				СтруктураПараметров.Вставить("ОДПН", ОДПН);
				СтруктураПараметров.Вставить("СуммаПН", СуммаПН);
				
