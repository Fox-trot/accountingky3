		ТабличныйДокумент.Вывести(ОбластьИтого);
		
		Если НомерЛиста = КолвоЛистов Тогда
			ОбластьКомментарий = Макет.ПолучитьОбласть("Комментарий");
			
			ДанныеПечати.Вставить("ОДПН", ТаблицаДокумента.Итог("ОДПН"));
			ДанныеПечати.Вставить("СуммаПН", ТаблицаДокумента.Итог("СуммаПН"));
			ЗаполнитьЗначенияСвойств(ОбластьКомментарий.Параметры,ДанныеПечати);
			ТабличныйДокумент.Вывести(ОбластьКомментарий);	
			
		КонецЕсли;
		
		ТабличныйДокумент.Вывести(ОбластьПодвал);		
		
		УправлениеПечатью.ЗадатьОбластьПечатиДокумента(ТабличныйДокумент, НомерСтрокиНачало, ОбъектыПечати, Шапка.Ссылка);
	КонецЦикла;
	
	Возврат ТабличныйДокумент;
	
