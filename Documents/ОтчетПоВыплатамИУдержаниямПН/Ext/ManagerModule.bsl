					СтруктураПараметров.КодСтраны1 = Сред(Строка(КодСтраны),1,1);
					СтруктураПараметров.КодСтраны2 = Сред(Строка(КодСтраны),2,1);
					СтруктураПараметров.КодСтраны3 = Сред(Строка(КодСтраны),3,1);
					
				КонецЕсли;
				
				ОДПН = ОДПН + СтрокаТаблицы.ОДПН;
				СуммаПН          = СуммаПН + СтрокаТаблицы.СуммаПН;
				
				ОбластьСтрока.Параметры.Заполнить(СтруктураПараметров);
				ТабличныйДокумент.Вывести(ОбластьСтрока);
				
