		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	СостоянияОССрезПоследних.ОсновноеСредство КАК ОсновноеСредство,
	СостоянияОССрезПоследних.Период КАК ДатаСписания
ПОМЕСТИТЬ ВТ_СписанныеОС
{ВЫБРАТЬ
	ОсновноеСредство.*,
	ДатаСписания}
ИЗ
	РегистрСведений.СостоянияОС.СрезПоследних(, ) КАК СостоянияОССрезПоследних
ГДЕ
	СостоянияОССрезПоследних.Период &gt;= &amp;НачалоПериода
	И СостоянияОССрезПоследних.Состояние = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийОС.СнятоСУчета)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПараметрыУчетаОССрезПоследних.ИнвентарныйНомер КАК ИнвентарныйНомер,
	ВТ_СписанныеОС.ОсновноеСредство КАК ОсновноеСредство,
	ВТ_СписанныеОС.ДатаСписания КАК ДатаСписания,
	ПараметрыУчетаОССрезПоследних.ПервоначальнаяСтоимость КАК КредитАктивы,
	Амортизация.СуммаОборот КАК ДебетНакопленнойАмортизации,
	Расходы.СуммаОборот КАК ДебетРасходы
{ВЫБРАТЬ
	ИнвентарныйНомер,
	ОсновноеСредство.*,
	ДатаСписания,
	КредитАктивы,
	ДебетНакопленнойАмортизации,
	ДебетРасходы}
ИЗ
	ВТ_СписанныеОС КАК ВТ_СписанныеОС
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ПараметрыУчетаОС.СрезПоследних(, ) КАК ПараметрыУчетаОССрезПоследних
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Хозрасчетный.ОборотыДтКт(, , , , , , , ) КАК Расходы
			ПО ПараметрыУчетаОССрезПоследних.СчетУчета = Расходы.СчетКт
				И ПараметрыУчетаОССрезПоследних.СчетУчета.ПарныйСчет &lt;&gt; Расходы.СчетДт
				И ПараметрыУчетаОССрезПоследних.СчетУчета &lt;&gt; Расходы.СчетДт
			ЛЕВОЕ СОЕДИНЕНИЕ РегистрБухгалтерии.Хозрасчетный.ОборотыДтКт(, , , , , , ЗНАЧЕНИЕ(ПланВидовХарактеристик.ВидыСубконтоХозрасчетные.ОсновныеСредства), ) КАК Амортизация
			ПО ПараметрыУчетаОССрезПоследних.СчетУчета.ПарныйСчет = Амортизация.СчетКт
		ПО ВТ_СписанныеОС.ОсновноеСредство = ПараметрыУчетаОССрезПоследних.ОсновноеСредство</query>
		<autoFillFields>false</autoFillFields>
	</dataSet>
