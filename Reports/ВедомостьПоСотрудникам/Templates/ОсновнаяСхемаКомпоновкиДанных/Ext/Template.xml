		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	Начисления.Организация,
	Начисления.ФизЛицо,
	ВЫБОР
		КОГДА Начисления.ПериодРегистрации МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
			ТОГДА Начисления.Результат
		ИНАЧЕ 0
	КОНЕЦ КАК Результат,
	Начисления.Результат КАК РезультатСНачалаГода,
	ВЫБОР
		КОГДА Начисления.ПериодРегистрации МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
				И Начисления.ВидРасчета.СпособРасчета = Значение(Перечисление.СпособыРасчетаНачислений.ОплатаБольничного)
			ТОГДА Начисления.Результат
		ИНАЧЕ 0
	КОНЕЦ КАК РезультатБольничныйЛист,
	ВЫБОР
		КОГДА Начисления.ВидРасчета.СпособРасчета = Значение(Перечисление.СпособыРасчетаНачислений.ОплатаБольничного)
			ТОГДА Начисления.Результат
		ИНАЧЕ 0
	КОНЕЦ КАК РезультатБольничныйЛистСНачалаГода
ИЗ
	РегистрРасчета.Начисления КАК Начисления
ГДЕ
	Начисления.ПериодРегистрации МЕЖДУ &amp;НачалоГода И &amp;КонецПериода</query>
