		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ДоверенностьТовары.Ссылка.Дата,
	ДоверенностьТовары.Ссылка.ДатаДействия,
	ДоверенностьТовары.Ссылка.Контрагент,
	ДоверенностьТовары.Ссылка.ФизЛицо,
	ДоверенностьТовары.Ссылка.Комментарий,
	ДоверенностьТовары.Номенклатура,
	ДоверенностьТовары.Количество
ИЗ
	Документ.Доверенность.Товары КАК ДоверенностьТовары
ГДЕ
	ДоверенностьТовары.Ссылка.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И ДоверенностьТовары.НомерСтроки = 1
	И НЕ ДоверенностьТовары.Ссылка.ПометкаУдаления</query>
