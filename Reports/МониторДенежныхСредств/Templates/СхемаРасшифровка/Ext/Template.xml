	NULL,
	ВременнаяТаблицаОстатки.СуммаКонечныйОстатокДт,
	ВременнаяТаблицаОстатки.СуммаКонечныйОстатокКт,
	ВременнаяТаблицаОстатки.Организация,
	NULL,
	"Конечное сальдо:",
	NULL,
	NULL,
	NULL,
	2,
	ВременнаяТаблицаОстатки.ВалютнаяСуммаКонечныйОстатокДт,
	ВременнаяТаблицаОстатки.ВалютнаяСуммаКонечныйОстатокКт,
	NULL
