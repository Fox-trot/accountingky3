ВЫБРАТЬ
	ВременнаяТаблицаОстатки.Субконто1,
	NULL,
	СУММА(ВременнаяТаблицаОстатки.СуммаОборотДт),
	СУММА(ВременнаяТаблицаОстатки.СуммаОборотКт),
	ВременнаяТаблицаОстатки.Организация,
	NULL,
	"Обороты за период:",
	NULL,
	NULL,
	NULL,
	3,
	ВременнаяТаблицаОстатки.ВалютнаяСуммаОборотДт,
	ВременнаяТаблицаОстатки.ВалютнаяСуммаОборотКт,
	NULL
ИЗ
	ВременнаяТаблицаОстатки КАК ВременнаяТаблицаОстатки

СГРУППИРОВАТЬ ПО
	ВременнаяТаблицаОстатки.Организация,
	ВременнаяТаблицаОстатки.Субконто1,
	ВременнаяТаблицаОстатки.ВалютнаяСуммаОборотДт,
	ВременнаяТаблицаОстатки.ВалютнаяСуммаОборотКт

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ХозрасчетныйДвиженияССубконто.СубконтоКт1,
	ХозрасчетныйДвиженияССубконто.СубконтоДт1,
	ВЫБОР
		КОГДА ХозрасчетныйДвиженияССубконто.Сумма &lt; 0
			ТОГДА -ХозрасчетныйДвиженияССубконто.Сумма
		ИНАЧЕ 0
	КОНЕЦ,
	ВЫБОР
		КОГДА ХозрасчетныйДвиженияССубконто.Сумма &gt; 0
			ТОГДА ХозрасчетныйДвиженияССубконто.Сумма
		ИНАЧЕ 0
	КОНЕЦ,
	ХозрасчетныйДвиженияССубконто.Организация,
	ХозрасчетныйДвиженияССубконто.Период,
	ХозрасчетныйДвиженияССубконто.Регистратор,
	ХозрасчетныйДвиженияССубконто.Регистратор.Операция,
	ХозрасчетныйДвиженияССубконто.Регистратор.Курс,
	ХозрасчетныйДвиженияССубконто.СчетКт,
	4,
	ВЫБОР
		КОГДА ХозрасчетныйДвиженияССубконто.Сумма &lt; 0
			ТОГДА ВЫБОР
					КОГДА ХозрасчетныйДвиженияССубконто.ВалютаДт ЕСТЬ NULL 
						ТОГДА -ХозрасчетныйДвиженияССубконто.Сумма
					ИНАЧЕ ХозрасчетныйДвиженияССубконто.ВалютнаяСуммаДт
				КОНЕЦ
		ИНАЧЕ 0
	КОНЕЦ,
	ВЫБОР
		КОГДА ХозрасчетныйДвиженияССубконто.Сумма &gt; 0
			ТОГДА ВЫБОР
					КОГДА ХозрасчетныйДвиженияССубконто.ВалютаКт ЕСТЬ NULL 
						ТОГДА ХозрасчетныйДвиженияССубконто.Сумма
					ИНАЧЕ ХозрасчетныйДвиженияССубконто.ВалютнаяСуммаКт
